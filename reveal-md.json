{
  "separator": "^\\n---\\n$",
  "verticalSeparator": "^\\n----\\n$",
  "theme": "simple",
  "highlightTheme": "monokai",
  "css": "css/custom.css",
  "scripts": "js/auto-scale.js",
  "assetsDir": "assets",
  "title": "TeacherSlide",
  "revealOptions": {
    "width": 1280,
    "height": 720,
    "margin": 0.04,
    "minScale": 0.2,
    "maxScale": 2.0,
    "controls": true,
    "progress": true,
    "slideNumber": true,
    "history": true,
    "keyboard": true,
    "overview": true,
    "center": true,
    "touch": true,
    "loop": false,
    "rtl": false,
    "fragments": true,
    "embedded": false,
    "help": true,
    "showNotes": false,
    "autoPlayMedia": null,
    "preloadIframes": null,
    "autoSlide": 0,
    "mouseWheel": false,
    "transition": "slide",
    "transitionSpeed": "default",
    "backgroundTransition": "fade"
  }
}
